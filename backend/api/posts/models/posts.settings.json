{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "user": {
      "via": "posts",
      "plugin": "users-permissions",
      "model": "user"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "webSite": {
      "type": "text"
    },
    "socialSite": {
      "type": "string"
    }
  }
}
